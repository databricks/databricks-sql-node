{
  "name": "@databricks/sql",
  "version": "0.1.8-beta.1",
  "description": "Driver for connection to Databricks SQL via Thrift API.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "prepack": "npm run build",
    "prepublish": "npm run build",
    "e2e": "nyc --reporter=lcov --report-dir=coverage_e2e mocha 'tests/e2e/**/*.test.js' --timeout=300000",
    "test": "nyc --reporter=lcov --report-dir=coverage_unit mocha 'tests/unit/**/*.test.js'",
    "coverage": "codecov",
    "build": "tsc",
    "watch": "tsc -w",
    "prettier:fix": "prettier . --write",
    "prettier:check": "prettier . --check"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/databricks/databricks-sql-nodejs/"
  },
  "bugs": {
    "url": "https://github.com/databricks/databricks-sql-nodejs/issues"
  },
  "homepage": "https://github.com/databricks/databricks-sql-nodejs#readme",
  "keywords": [
    "databricks sql",
    "hiveserver2",
    "apache",
    "hive",
    "hs2",
    "client",
    "thrift",
    "nodejs",
    "js",
    "hive",
    "hive js"
  ],
  "license": "Apache 2.0",
  "devDependencies": {
    "@types/node": "^18.0.0",
    "chai": "^4.3.6",
    "codecov": "^3.8.3",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "commander": "^9.3.0",
    "node-int64": "^0.4.0",
    "thrift": "^0.16.0"
  }
}
