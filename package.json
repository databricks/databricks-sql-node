{
  "name": "databricks-sql-node",
  "version": "0.1.8-beta.1",
  "description": "Driver for connection to Databricks SQL via Thrift API.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "scripts": {
    "e2e": "mocha 'tests/e2e/**/*.test.js' --timeout=300000",
    "test": "nyc --reporter=lcov mocha 'tests/unit/**/*.test.js'",
    "coverage": "codecov",
    "build": "tsc",
    "watch": "tsc -w",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/databricks/databricks-sql-node/"
  },
  "bugs": {
    "url": "https://github.com/databricks/databricks-sql-node/issues"
  },
  "homepage": "https://github.com/databricks/databricks-sql-node#readme",
  "keywords": [
    "databricks sql",
    "hiveserver2",
    "apache",
    "hive",
    "hs2",
    "client",
    "thrift",
    "nodejs",
    "js",
    "hive",
    "hive js"
  ],
  "license": "Apache 2.0",
  "devDependencies": {
    "@types/node": "^13.1.4",
    "chai": "^4.2.0",
    "codecov": "^3.7.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.1.0",
    "mocha": "^8.4.0",
    "nyc": "^15.0.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "commander": "^9.2.0",
    "node-int64": "^0.4.0",
    "thrift": "0.12.0"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  }
}
